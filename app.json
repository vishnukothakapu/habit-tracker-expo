{
  "expo": {
    "name": "HabitFlow",
    "slug": "smarthabit-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/habit_logo.png",
    "scheme": "smarthabittracker",
    "owner": "vishnukothakapu27",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/habit_logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.vishnukothakapu27.smarthabittracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/habit_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain"
          
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f1454656-b82b-4654-b929-b732835cb96c"
      }
    }
  }
}
